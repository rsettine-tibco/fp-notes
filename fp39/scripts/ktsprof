#!/bin/ksh

if [ "$1" = "" ]
then
    echo "1st argument : username missing"
    return
fi

if [ "$2" = "" ]
then
    echo "2nd argument : project missing"
    return
fi

if [ "$3" = "" ]
then
    echo "3rd argument : vnc server missing missing"
    return
fi

if [ "$4" = "" ]
then
    echo "4th argument : vnc number missing"
    return
fi

export DISPLAY=$3:$4.0

if [ ! -d /opt/kabira/users/$1 ]
then
    echo "/opt/kabira/users/$1 doesn't exist"
    echo "creating /opt/kabira/users/$1 ..."
    cd /opt/kabira/users
    mkdir $1
    
fi

if [ ! -d /opt/kabira/users/$1/$2 ]
then
	echo "/opt/kabira/users/$1/$2 doesn't exist"
    echo "creating /opt/kabira/users/$1/$2 ..."
    cd /opt/kabira/users/$1
    mkdir $2
    
fi

export SW_HOME=/opt/kabira/users/$1

cd ~/

#-------------------------------------------------------------------------------
# Functions
#-------------------------------------------------------------------------------
Addpath()
{  
#    if [ ! -d $1 ]
#    then
#         echo "Addpath: $1 does not exist"
#    fi
	PATH=$PATH:$1 
	export PATH

	#PATH=$1:$PATH
	#export PATH
}

AddLibraryPath()
{
#    if [ ! -d $1 ]
#    then
#        echo "AddLibraryPath: $1 does not exist"
#    fi
	LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$1
	export LD_LIBRARY_PATH

	#LD_LIBRARY_PATH=$1:$LD_LIBRARY_PATH
	#export LD_LIBRARY_PATH
}

#****************************************************************
#********************* KABIRA Technologies **********************
#****************************************************************
stty erase ^?
