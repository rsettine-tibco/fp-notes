unset PATH
unset LD_LIBRARY_PATH
unset OPENSSL
unset TNS_ADMIN
unset SDD_BIN
unset SW_PLATFORM

PLAT=`/bin/uname`
case ${PLAT} in
	Linux)
		export SW_PLATFORM=linux
		export USERNAME=`/usr/bin/whoami`
		export HOST=`/bin/uname -n`
		if [ "$HOST" = "cheetah" ]
		then
			export OPENSSL=$SW_HOME/3rdparty/$SW_PLATFORM/openssl/0.9.7c_ia64
			Addpath /opt/kabira/3rdparty/$SW_PLATFORM/dmake/4.1_ia64
			Addpath /opt/kabira/3rdparty/$SW_PLATFORM/gnu/1.1/bin
		else
			#export OPENSSL=$SW_HOME/3rdparty/$SW_PLATFORM/openssl/1.0.2j_x86_64
			Addpath /opt/kabira/3rdparty/$SW_PLATFORM/dmake/4.1_x86_64
#Addpath /opt/kabira/3rdparty/$SW_PLATFORM/dmake/4.11_x86_64/bin
			Addpath /opt/kabira/3rdparty/$SW_PLATFORM/gnu/1.1/bin_x86_64
		fi
	;;
        HP-UX)
		export SW_PLATFORM=hpux
		export USERNAME=`/bin/whoami`
	;;
	*)
		export SW_PLATFORM=sol
		export USERNAME=`/usr/ucb/whoami`
		export HOST=`/usr/bin/uname -n`
		export OPENSSL=$SW_HOME/3rdparty/$SW_PLATFORM/openssl/0.9.8o_64
		Addpath /opt/kabira/3rdparty/$SW_PLATFORM/dmake/4.1_64
		Addpath /usr/local/bin 
		Addpath /opt/kabira/3rdparty/$SW_PLATFORM/gnu/1.1/bin
	;;
esac

